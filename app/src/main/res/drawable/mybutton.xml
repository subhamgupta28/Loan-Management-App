<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="6dip" />
            <stroke android:width="1dip" android:color="#FB68CB" />
            <gradient android:angle="-90" android:startColor="#F66296" android:endColor="#0C99DF"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="6dip" />
            <stroke android:width="1dip" android:color="#FB68CB" />
            <solid android:color="#F66296"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="6dip" />
            <stroke android:width="1dip" android:color="#FB68CB" />
            <gradient android:angle="-90" android:startColor="#F66296" android:endColor="#76CDFB" />
        </shape>
    </item>
</selector>